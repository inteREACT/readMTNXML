{
  "name": "readmtnxml",
  "version": "1.0.1",
  "description": "Javascript function to read a MTN.XML feed and convert it to a JavaScript object for use within your web page",
  "main": "readMTNXML.js",
  "scripts": {
    "test": "mocha --require reify --require jsdom-global/register 'test.js'",
    "prod:build": "NODE_PATH=./ NODE_ENV=production browserify -t [ babelify --presets [ es2015 ] ] -g envify readMTNXML.js | NODE_ENV=production node_modules/uglify-js-harmony/bin/uglifyjs --define --compress drop_console -o dist/readMTNXML.js --preamble \"/*\nreadMTNXML $(echo $npm_package_version)\nCopyright 2017-$(date +\"%Y\") PURPLE HALO LIMITED\n*/\"",
    "dev:build:watch": "NODE_ENV=development watchify -v -t [ babelify --sourceMapsAbsolute ] -g envify --debug readMTNXML.js --outfile ./dist/readMTNXML.js",
    "versionbump:fix": "npm version patch --no-git-tag-version",
    "versionbump:feature": "npm version minor --no-git-tag-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inteREACT/readMTNXML.git"
  },
  "keywords": [
    "MTN.XML",
    "ski",
    "resort",
    "snow",
    "report",
    "reader"
  ],
  "author": "Purple Halo Limited",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inteREACT/readMTNXML/issues"
  },
  "homepage": "https://github.com/inteREACT/readMTNXML#readme",
  "dependencies": {
    "axios": "^0.16.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "chai": "^3.5.0",
    "dirty-chai": "^1.2.2",
    "envify": "^4.0.0",
    "jsdom": "^9.12.0",
    "jsdom-global": "^2.1.1",
    "mocha": "^3.2.0",
    "reify": "^0.6.7",
    "uglify-js-harmony": "^2.7.5",
    "watchify": "^3.9.0"
  }
}
